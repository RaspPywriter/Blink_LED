setEx = set(['Python', 'Java', 'Angular'])
print('Python' in setEx)
setExam = setEx.copy()
setEx.add('C#')
print(setEx)
setExam.issuperset(setEx)
setEx.remove('Java')
print('setExam ', setExam)
print('setEx ', setEx)
print('intersection ', setExam & setEx)
setEx.add('HTML')
setEx.update(['React', 'C', 'C++'])
print(setEx)
setEx.pop()
print(setEx)
